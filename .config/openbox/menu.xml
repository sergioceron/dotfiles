				<execute>~/bin/thunderbird</execute>
			</action>
		</item>
		<item label="transmission">
			<action name="Execute">
				<execute>
					transmission-gtk
				</execute>
			</action>
		</item>
		<item label="vncviewer">
			<action name="Execute">
				<execute>
