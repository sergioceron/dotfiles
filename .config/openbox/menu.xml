		</item>
		<separator/>
		<item label="lock">
			<action name="Execute">
				<execute>
					xscreensaver-command -lock
				</execute>
			</action>
		</item>
		<item label="suspend">
			<action name="Execute">
				<execute>
					sudo pm-suspend
				</execute>
			</action>
		</item>
		<separator/>
		<item label="logout">
			<action name="Exit"/>
		</item>
