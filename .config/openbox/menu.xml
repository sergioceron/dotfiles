	<menu id="apps-net-menu" label="internet">
		<item label="chromium">
			<action name="Execute">
				<execute>
					chromium --disk-cache-dir=&quot;/tmp/chromium-cache&quot; --memory-model=low --allow-outdated-plugins
				</execute>
			</action>
		</item>
		<item label="filezilla">
			<action name="Execute">
				<execute>
					filezilla
				</execute>
			</action>
		</item>
		<item label="firefox">
			<action name="Execute">
				<command>
					firefox
				</command>
				<startupnotify>
					<enabled>
						yes
					</enabled>
					<wmclass>
						Firefox
					</wmclass>
				</startupnotify>
			</action>
		</item>
		<item label="gmail notify">
			<action name="Execute">
				<execute>gmail-notify</execute>
