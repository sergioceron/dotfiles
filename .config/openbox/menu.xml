				<command>
					pidgin
				</command>
				<startupnotify>
					<enabled>
						yes
					</enabled>
				</startupnotify>
			</action>
		</item>
		<item label="skype">
			<action name="Execute">
				<execute>
					skype
				</execute>
			</action>
		</item>
		<item label="thunderbird">
